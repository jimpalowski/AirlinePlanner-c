<h1>To Do List</h1>
<hr />
<h2>Cities:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var city in Model)
    {
      <li><a href="/cities/@city.GetId()">@city.GetName()</a></li>
      <form action='/cities/@city.GetId()/delete' method="post">
            <button class="btn" type="submit">Delete Category</button>
            </form>
    }
  </ul>
}

<h4><a href="/cities/new">Add a new city</a></h4>
<h4><a href="/">Return home</a></h4>
